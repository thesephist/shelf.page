{
  "name": "shelf",
  "version": "0.1.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev": "npm run asset-bundler-initial && concurrently --kill-others 'npm:nodemon' 'npm:asset-bundler-watch'",
    "build": "npm run asset-bundler-production",
    "nodemon": "nodemon server.js --watch ./",
    "asset-bundler-initial": "node _remake-asset-bundler/initial-build.js",
    "asset-bundler-production": "node _remake-asset-bundler/initial-build.js production",
    "asset-bundler-watch": "node _remake-asset-bundler/",
    "build-selftest": "parcel build ./_remake/tests/remake-init-gen.js -d ./_remake/tests/ -o remake-init.js",
    "selftest": "npm run build-selftest && echo 'The self test page is ready. Go open the file at ./_remake/tests/selftest.html in your favorite browser.'",
    "clean": "rm -rf .cache && rm -rf _remake/dist"
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/",
      "_remake/.sessions",
      "_remake/client-side",
      "_remake/dist",
      "_remake-data",
      "_remake-uploads",
      "node_modules/",
      "app"
    ],
    "ext": "html,hbs,sass,css,js,png,gif,jpg,json"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "alias": {
    "remake-framework": "./_remake/client-side/index.js"
  },
  "dependencies": {
    "archiver": "^3.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "browserslist": "^4.12.1",
    "camelcase": "^5.3.1",
    "caniuse-lite": "^1.0.30001088",
    "change-case": "^3.1.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "crostini": "^1.0.3",
    "deep-extend": "^0.6.0",
    "deepdash-es": "^4.2.5",
    "delegated-events": "^1.1.0",
    "directory-tree": "^2.2.3",
    "esm": "^3.2.25",
    "express": "^4.17.0",
    "express-fileupload": "^1.1.6",
    "express-session": "^1.16.2",
    "extract-zip": "^1.6.7",
    "glob": "^7.1.4",
    "handlebars": "^4.5.3",
    "handlebars-helpers": "^0.10.0",
    "jsonfile": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mailgun-js": "^0.22.0",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql": "^2.17.1",
    "nanoid": "^2.0.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path-match": "^1.2.4",
    "rimraf": "3.0.0",
    "session-file-store": "^1.3.1",
    "shelljs": "^0.8.4",
    "sortablejs": "^1.10.1",
    "upath": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "chokidar": "^3.2.1",
    "concurrently": "^4.1.0",
    "copy-and-watch": "^0.1.4",
    "nodemon": "^1.19.1",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.23.0",
    "shelljs": "^0.8.3"
  }
}
